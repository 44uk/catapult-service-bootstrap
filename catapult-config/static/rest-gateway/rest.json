{
  "network": {
    "name": "mijinTest",
    "description": "catapult development network"
  },

  "port": 3000,
  "crossDomainHttpMethods": ["GET", "POST", "PUT", "OPTIONS"],
  "clientPrivateKey": "2605DD767CCAE0350483D3EFD9D8AE9DA0C66C121B521B1F9D9336DFE33266A4",
  "extensions": ["aggregate", "multisig", "namespace", "transfer"],

  "db": {
    "url": "mongodb://db:27017/",
    "name": "catapult",
    "pageSizeMin": 10,
    "pageSizeMax": 100,
    "pageSizeStep": 25,
    "maxConnectionAttempts": 5,
    "baseRetryDelay": 500
  },

  "apiNode": {
    "host": "peer1",
    "port": 7900,
    "publicKey": "D3D5874EC475E9F66E435C5323DC55193CA734BE640629907B73FA345A258BC8"
  },

  "websocket": {
    "mq": {
      "host": "api1",
      "port": 7902
    },
    "allowOptionalAddress": true
  },

  "logging": {
    "console": {
      "colorize": true,
      "level": "verbose",
      "handleExceptions": true,
      "prettyPrint": true,
      "timestamp": true
    },
    "file": {
      "level": "verbose",
      "handleExceptions": true,
      "prettyPrint": true,
      "timestamp": true,
      "filename": "catapult-rest.log",
      "maxsize": 20971520,
      "maxFiles": 100
    }
  }
}
